<div class="container">
  <div class="row">
    <div class="col s12">
      <h1>Welcome</h1>
      <br>
    </div>
  </div>
  <div class="row">
    <div class="col s5">
      <h3>Checked In</h3>
    </div>
    <div class="col s2">
    </div>
    <div class="col s5">
      <h3>Checked out</h3>
    </div>
  </div>
  <div class="row">
    <div class="col s5">
      <ul>
        {{#each students}}
        <li>
          <span class="glyphicons glyphicons-plus-sign"></span>
          <h4 class="truncate">
            <b>Name:{{this.lastName}}</b> Student #: {{this.studentNumber}}</h4>
        </li>
        {{/each}}
      </ul>
    </div>
    <div class="col s2">
    </div>
    <div class="col s5">
      {{#each students}}
      <li>
        <span class="glyphicons glyphicons-minus-sign"></span>
        <h4 class="truncate">
          <b>Name:{{this.lastName}}</b> Student #: {{this.studentNumber}}</h4>
      </li>
      {{/each}}
    </div>
  </div>

  <div class="row">
    <div class="col">
      <div class="row">
          <div class="input-field col s6">
            <input placeholder="Placeholder" id="first_name" type="text" class="validate">
            <label for="first_name">First Name</label>
          </div>
          <div class="input-field col s6">
            <input id="last_name" type="text" class="validate">
            <label for="last_name">Last Name</label>
          </div>
      </div>
  <div class="row">
    <div class="input-field col s12">
      <input disabled value="I am not editable" id="disabled" type="text" class="validate">
      <label for="disabled">Disabled</label>
    </div>
  </div>
      <br>
  <div class="row">
    <div class="col">
      <div class="row">
          <div class="col s6">
            <button class="btn waves-effect waves-light" type="submit" name="action">Check In
            <i class="material-icons right">Check In</i>
            </button>
          </div>
          <div class="col">
            <button class="btn waves-effect waves-light" type="submit" name="action">Submit
              <i class="material-icons right">Check Out</i>
            </button>
          </div>
      </div>
    </div>
  </div>
</div>